<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="847.0" prefWidth="1512.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.ReportsController">
   <children>
      <!-- Background -->
      <ImageView fitHeight="850.0" fitWidth="1700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pngs/reports_bg.png" />
         </image>
      </ImageView>

      <!-- Reports Table -->
      <TableView fx:id="reportsTable" layoutX="136.0" layoutY="220.0" prefHeight="557.0" prefWidth="1257.0">
         <columns>
            <TableColumn fx:id="firstNameCol" prefWidth="250.0" text="First Name" />
            <TableColumn fx:id="lastNameCol" prefWidth="250.0" text="Last Name" />
            <TableColumn fx:id="healthGoalCol" prefWidth="250.0" text="Health Goal" />
            <TableColumn fx:id="initialWeightCol" prefWidth="250.0" text="Initial Weight" />
            <TableColumn fx:id="goalWeightCol" prefWidth="250.0" text="Goal Weight" />
         </columns>
      </TableView>

      <!-- Invisible Back Button (assuming image overlay has clickable area) -->
      <Button fx:id="backBtn" layoutX="40.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleBack" opacity="0.0" prefHeight="47.0" prefWidth="49.0" />
   </children>
</AnchorPane>
