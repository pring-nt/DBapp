<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.ListMembersController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pngs/list_members.png" />
         </image>
      </ImageView>

      <Button fx:id="btnBack" layoutX="231.0" layoutY="344.0" mnemonicParsing="false" onAction="#handleBack" opacity="0.0" prefHeight="35.0" prefWidth="139.0" text="Button" />

      <TableView fx:id="membersTable" layoutX="32.0" layoutY="100.0" prefHeight="232.0" prefWidth="538.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="colFirstName" prefWidth="75.0" text="First Name" />
            <TableColumn fx:id="colLastName" prefWidth="75.0" text="Last Name" />
            <TableColumn fx:id="colEmail" prefWidth="75.0" text="Email" />
            <TableColumn fx:id="colContact" prefWidth="75.0" text="Contact #" />
            <TableColumn fx:id="colMembership" prefWidth="75.0" text="Membership Type" />
            <TableColumn fx:id="colStartDate" prefWidth="75.0" text="Start Date" />
            <TableColumn fx:id="colEndDate" prefWidth="75.0" text="End Date" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
