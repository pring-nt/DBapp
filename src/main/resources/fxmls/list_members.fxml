<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1020.0" prefWidth="1980.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.ListMembersController">
   <children>
      <ImageView fitHeight="1000.0" fitWidth="1700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pngs/list_members.png" />
         </image>
      </ImageView>

      <Button fx:id="btnBack" layoutX="64.0" layoutY="62.0" mnemonicParsing="false" onAction="#handleBack" opacity="0.0" prefHeight="45.0" prefWidth="32.0" text="Button" />

      <TableView fx:id="membersTable" layoutX="536.0" layoutY="282.0" prefHeight="498.0" prefWidth="823.0">
         <columns>
            <TableColumn fx:id="colId" prefWidth="60.0" text="ID" />
            <TableColumn fx:id="colFirstName" text="First Name" />
            <TableColumn fx:id="colLastName" text="Last Name" />
            <TableColumn fx:id="colEmail" prefWidth="150.0" text="Email" />
            <TableColumn fx:id="colContact" prefWidth="120.0" text="Contact #" />
            <TableColumn fx:id="colMembership" prefWidth="120.0" text="Membership Type" />
            <TableColumn fx:id="colStartDate" prefWidth="105.0" text="Start Date" />
            <TableColumn fx:id="colEndDate" prefWidth="105.0" text="End Date" />
         </columns>
      </TableView>
      <Button layoutX="114.0" layoutY="366.0" mnemonicParsing="false" onAction="#handleEntryForm" opacity="0.0" prefHeight="59.0" prefWidth="197.0" />
      <Button layoutX="114.0" layoutY="531.0" mnemonicParsing="false" onAction="#handleRemove" opacity="0.0" prefHeight="59.0" prefWidth="197.0" />
      <Button layoutX="104.0" layoutY="728.0" mnemonicParsing="false" onAction="#handleUpdate" opacity="0.0" prefHeight="59.0" prefWidth="197.0" />
   </children>
</AnchorPane>
