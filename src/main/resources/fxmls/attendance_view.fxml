<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1020.0" prefWidth="1980.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.AttendanceViewController">
   <children>
      <ImageView fitHeight="1000.0" fitWidth="1700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pngs/att_table.png" />
         </image>
      </ImageView>
      <TableView fx:id="attendanceTable" layoutX="593.0" layoutY="236.0" prefHeight="426.0" prefWidth="592.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="150.0" text="Attendance ID" />
          <TableColumn fx:id="colDatetime" prefWidth="150.0" text="Date &amp; Time" />
            <TableColumn fx:id="colMember" prefWidth="140.0" text="Member" />
            <TableColumn fx:id="colClassType" prefWidth="150.0" text="Class" />
        </columns>
      </TableView>
      <Button fx:id="btnBack" layoutX="64.0" layoutY="62.0" mnemonicParsing="false" onAction="#handleBack" opacity="0.0" prefHeight="40.0" prefWidth="31.0" text="Button" />
      <Button layoutX="111.0" layoutY="191.0" mnemonicParsing="false" onAction="#handleCheck" opacity="0.0" prefHeight="64.0" prefWidth="192.0" />
   </children>
</AnchorPane>
