      <Button layoutX="113.0" layoutY="174.0" mnemonicParsing="false" onAction="#handleListTrainers" opacity="0.0" prefHeight="84.0" prefWidth="202.0" />
      <Button layoutX="117.0" layoutY="529.0" mnemonicParsing="false" onAction="#handleRemove" opacity="0.0" prefHeight="84.0" prefWidth="202.0" />
      <Button layoutX="106.0" layoutY="716.0" mnemonicParsing="false" onAction="#handleUpdate" opacity="0.0" prefHeight="84.0" prefWidth="202.0" />
   </children>
</AnchorPane>
