<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="1020.0" prefWidth="1980.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.TrainerEntryController">
   <children>
      <ImageView fitHeight="1000.0" fitWidth="1700.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../pngs/trainer_entry.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtFirstName" layoutX="900.0" layoutY="215.0" prefHeight="30.0" prefWidth="400.0" />
      <TextField fx:id="txtLastName" layoutX="900.0" layoutY="295.0" prefHeight="30.0" prefWidth="400.0" />
      <TextField fx:id="txtRecord" layoutX="960.0" layoutY="620.0" prefHeight="30.0" prefWidth="340.0" />
      <ComboBox fx:id="cmbType" layoutX="900.0" layoutY="380.0" prefHeight="30.0" prefWidth="400.0" />
      <Button fx:id="btnBack" layoutX="160.0" layoutY="347.0" mnemonicParsing="false" onAction="#handleBack" opacity="0.0" prefHeight="25.0" prefWidth="117.0" text="Button" />
      <Button fx:id="btnSave" layoutX="315.0" layoutY="347.0" mnemonicParsing="false" onAction="#handleSave" opacity="0.0" prefHeight="25.0" prefWidth="117.0" text="Button" />
