<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gymdb.controller.MainController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@main_bg.png" />
         </image>
      </ImageView>
      <Button fx:id="btnAllMembers" layoutX="50.0" layoutY="202.0" mnemonicParsing="false" onAction="#handleAllMembers" opacity="0.0" prefHeight="99.0" prefWidth="105.0" text="Button" />
      <Button fx:id="btnEntryForm" layoutX="180.0" layoutY="202.0" mnemonicParsing="false" onAction="#handleEntryForm" opacity="0.0" prefHeight="99.0" prefWidth="105.0" text="Button" />
      <Button fx:id="btnRemove" layoutX="313.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleRemove" opacity="0.0" prefHeight="99.0" prefWidth="105.0" text="Button" />
      <Button fx:id="btnUpdate" layoutX="445.0" layoutY="200.0" mnemonicParsing="false" onAction="#handleUpdate" opacity="0.0" prefHeight="99.0" prefWidth="105.0" text="Button" />
   </children>
</AnchorPane>
